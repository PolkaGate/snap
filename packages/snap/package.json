{
  "name": "@polkagate/snap",
  "version": "2.3.1",
  "description": "A MetaMask Snap to interact with Polkadot ecosystem, a platform for cross-chain communication and scalability. Use your MetaMask wallet to access Polkadot dApps and tokens. No extra extension needed.",
  "repository": {
    "type": "git",
    "url": "https://github.com/polkagate/snap.git"
  },
  "license": "Apache-2.0",
  "main": "./dist/bundle.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "test": "jest --silent"
  },
  "dependencies": {
    "@dicebear/avatars": "latest",
    "@dicebear/avatars-identicon-sprites": "latest",
    "@metamask/key-tree": "^10.1.1",
    "@metamask/rpc-errors": "^7.0.3",
    "@metamask/snaps-sdk": "^9.3.0",
    "@polkadot/extension-chains": "^0.62.1",
    "@polkadot/networks": "^13.5.6 ",
    "@polkadot/types-known": "^16.4.8",
    "@polkadot/util-crypto": "^13.5.6 ",
    "@polkagate/apps-config": "^0.144.6",
    "buffer": "^6.0.3",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@lavamoat/allow-scripts": "^3.0.4",
    "@metamask/auto-changelog": "^4.0.0",
    "@metamask/eslint-config": "^14.0.0",
    "@metamask/eslint-config-jest": "^14.0.0",
    "@metamask/eslint-config-nodejs": "^14.0.0",
    "@metamask/eslint-config-typescript": "^14.0.0",
    "@metamask/snaps-cli": "^8.3.0",
    "@metamask/snaps-jest": "^9.4.0",
    "@metamask/utils": "^11.0.0",
    "@swc/core": "1.3.78",
    "@swc/jest": "^0.2.26",
    "@types/react": "18.2.4",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "deepmerge": "^4.3.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^60.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "rimraf": "^6.0.1",
    "through2": "^4.0.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": "^18.16 || >=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
